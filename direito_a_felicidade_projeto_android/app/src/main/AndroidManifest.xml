            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.direitoafelicidade.TelaSeries" />
        </activity>
        <activity
            android:name=".TelaSeries"
            android:label="@string/title_activity_tela_series"
            android:parentActivityName=".CategoriaActivity"
